<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stonebridge.mapper.mydb.GoodsMapper">
    <!--配置查询所有-->
    <select id="getGoodsById" resultType="com.stonebridge.domain.Goods">
        SELECT *
        FROM sp_goods
        WHERE goods_id = #{goods_id}
    </select>
</mapper>